---
import Standard from "../../layouts/standard.astro";
import "../../styles/base.css";
import Hero from "../../components/werk/hero.astro";
import Hero_img from "../../components/werk/hero_img.astro";
import Headline from "../../components/werk/headline.astro";
import Ipad from "../../components/werk/ipad.astro";
import Mobile from "../../components/werk/mobile.astro";
import Img from "../../components/werk/img.astro";
import Desktop from "../../components/werk/desktop.astro";
import IntroWhite from "../../components/gsap/gsap-intro-white.astro";
---

<Standard
  title="Studio Moondust - Elements Concept Interior"
  h1="Elements Concept Interior"
  description="Studio Moondust - projectpagina Elements Concept Interior"
  keywords="Elements Concept Interior, Interior, Webdesign, Development, E-commerce"
  og_img_url="/werk/elements/elements-hero-OG.webp"
  og_img_alt="Studio Moondust X Elements Concept Interior"
  og_site_name="Studio Moondust X Elements Concept Interior"
>
  <!-- intro-white -->
  <IntroWhite />
  <Hero
    heading="Internationale webshop voor Elements Interior"
    client="Elements Concept"
    services="webdesign, development, e-commerce"
    year="2022"
    project_url="www.interior.elementsconcept.com"
  />
  <Hero_img
    hero_img_src="/werk/elements/elements-hero-OG.webp"
    hero_img_alt="Marketing afbeelding met visuals van de website interior.elementsconcept.com"
  />
  <Headline
    headline="Elements Concept Interior maakt tijdloos interieus. Gemaakt met de hand en met een verhaal. Rechtstreeks van de bron"
  />
  <Ipad
    background="bg-[#FFF9F0]"
    vid_src="/werk/elements/vid-ipad.mp4"
    vid_title="iPad screencapture"
  />
  <Mobile
    vid_src="/werk/elements/vid-mobile.mp4"
    vid_title="Mobile screencapture websbop"
    img1_src="/werk/elements/elements-mobile-2.webp"
    img1_alt="Mobile screenshot interior.elementsconcept.com"
    img2_src="/werk/elements/elements-mobile-3.webp"
    img2_alt="Mobile screenshot interior.elementsconcept.com"
  />
  <Img
    img_src="/werk/elements/elements-section-img-bg.webp"
    img_alt="Marketing afbeelding met visuals van de website interior.elementsconcept.com"
  />
  <Desktop
    vid_src="/werk/elements/vid-desktop2.mp4"
    vid_title="Desktop screencapture interior.elementsconcept.com"
  />
  <Desktop
    vid_src="/werk/elements/vid-desktop.mp4"
    vid_title="Desktop screencapture interior.elementsconcept.com"
  />
  <section class="quote mx-auto max-w-screen-xl py-16 px-4 md:px-16 lg:py-32">
    <div class="pagination quote-text flex w-full items-center justify-center gap-4 sm:gap-12">
      <a href="../werk">&lt; werk</a> / <a href="../werk/vivaz"> next &gt;</a>
    </div><!-- /.quote -->
  </section>
</Standard>
