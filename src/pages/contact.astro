---
import Standard from "../layouts/standard.astro";
import IntroWhite from "../components/intro-white.astro";
import "../styles/base.css";
---

<Standard title="Studio Moondust - Contact" h1="Contact">
  <main>
    <!-- intro-white -->
    <IntroWhite />
    <section class="hero-intro px-4 pt-40 pb-16 font-jost md:pt-40 md:pb-24 lg:pt-56 lg:pb-36">
      <div class="hero-text">
        <h2 class="tricks relative inline">
          Hello! Have a project? <br /> Let's chat.
        </h2>
      </div>
    </section><!-- /.hero -->
    <section class="contact-form mx-auto max-w-[960px] px-4 pb-16 md:px-16 lg:pb-24">
